
package arrays_programs;

public class MaximumSumationOfArray 
{
	public static void main(String[] args)
	{
		int arr[]= {1,2,4,5,6,3};
		int max=0;
		for(int i=0; i<arr.length-1; i++)
		{
			int sum=0;
			for(int j=i+1; j<arr.length; j++)
			{
				sum=0;
				sum=(arr[i]+arr[j]);
				if(sum>max)
				{
					max=sum;
				}
			}
			
			//max=Math.max(sum,max);
		}
		System.out.println(max);		
	}
}
